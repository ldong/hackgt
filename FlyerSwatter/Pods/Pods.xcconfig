CLANG_CXX_LIBRARY = compiler-default
FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/TesseractOCRiOS/Products"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/AFNetworking" "${PODS_ROOT}/Headers/FHTTPClient" "${PODS_ROOT}/Headers/MKNetworkKit" "${PODS_ROOT}/Headers/MKNetworkKit/Categories" "${PODS_ROOT}/Headers/Reachability" "${PODS_ROOT}/Headers/TesseractOCRiOS"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/AFNetworking" -isystem "${PODS_ROOT}/Headers/FHTTPClient" -isystem "${PODS_ROOT}/Headers/MKNetworkKit" -isystem "${PODS_ROOT}/Headers/MKNetworkKit/Categories" -isystem "${PODS_ROOT}/Headers/Reachability" -isystem "${PODS_ROOT}/Headers/TesseractOCRiOS"
OTHER_LDFLAGS = -ObjC  -weak_library /usr/lib/libstdc++.6.0.9.dylib -lstdc++ -framework CFNetwork -framework CoreGraphics -framework Foundation -framework MobileCoreServices -framework Security -framework SystemConfiguration -framework TesseractOCR -framework UIKit
PODS_ROOT = ${SRCROOT}/Pods