CLANG_CXX_LIBRARY = compiler-default
FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/TesseractOCRiOS/Products"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/TesseractOCRiOS"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/TesseractOCRiOS"
OTHER_LDFLAGS = -ObjC  -weak_library /usr/lib/libstdc++.6.0.9.dylib -lstdc++ -framework Foundation -framework TesseractOCR -framework UIKit
PODS_ROOT = ${SRCROOT}/Pods